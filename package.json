{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npm run getTheme && npm run getPosts",
    "getTheme": "rm ./themes/landscape-plus -rf && git clone https://github.com/Trim21/landscape-plus.git ./themes/landscape-plus --depth=1",
    "getPosts": "rm ./source/_posts -rf && git clone https://github.com/Trim21/blog-posts.git ./source/_posts --depth=1",
    "server": "hexo s",
    "start": "node new.js",
    "build": "npm run preinstall && hexo clean && hexo g",
    "gen": "./node_modules/.bin/hexo clean && ./node_modules/.bin/hexo g"
  },
  "hexo": {
    "version": "3.9.0"
  },
  "dependencies": {
    "cli": "^1.0.0",
    "hexo": "^3.9.0",
    "hexo-autonofollow": "^1.0.1",
    "hexo-fs": "^2.0.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-feed": "^2.0.0",
    "hexo-generator-index": "^1.0.0",
    "hexo-generator-sitemap": "^1.2.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^2.0.0",
    "hexo-renderer-stylus": "^1.1.0",
    "hexo-server": "^0.3.3",
    "html-minifier": "^4.0.0"
  }
}
